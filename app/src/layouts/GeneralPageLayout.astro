---
import BaseLayout from './BaseLayout.astro';
import Content1 from 'fulldev-ui/blocks/Content1.astro';

const { frontmatter } = Astro.props;
---

<BaseLayout pageTitle={frontmatter.title}>
	<Content1 depth={1} heading={frontmatter.title} content=""image={{
			src: '/images/hs-logo.png',
			class: 'h-[100px] object-cover',
			alt: frontmatter.image?.alt || frontmatter.title,
		}}>
		<slot />
	</Content1>
</BaseLayout>
