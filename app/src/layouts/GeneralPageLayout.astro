---
import BaseLayout from './BaseLayout.astro';
import Content1 from '../components/FullDevCustomized/Content1.astro';

const { frontmatter } = Astro.props;
---

<BaseLayout pageTitle={frontmatter.title}>
	<Content1
		depth={1}
		heading={frontmatter.title}
		image={{
			src: frontmatter.image?.url || '/images/default.webp',
			class: 'h-[200px] object-cover',
			alt: frontmatter.image?.alt || 'A crosscut of a woodslab',
		}}
		link={{
			text: frontmatter.image?.from || 'FWStudio',
			href: frontmatter.image?.fromLink || 'https://www.pexels.com/@fwstudio-33348/',
		}}
	>
		<slot />
	</Content1>
</BaseLayout>
