{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "hashtagsheep-blog",
	"routes": [
		{
			"pattern": "hashtagsheep.com/*",
			"custom_domain": true
		}
	],
	"env": {
		"dev": {
			"route": "dev.hashtagsheep.com/*"
		}
	},
"compatibility_date": "2025-06-06",
"observability": {
	"enabled": true
},
"assets": {
	"directory": "./dist"
},
"placement": {
	"mode": "smart"
},
}